{
  "workflows": {
    "life-daily-brief": {
      "name": "daily-brief",
      "displayName": "Daily Briefing",
      "department": "life",
      "description": "Generate comprehensive daily briefing with weather, calendar, emails, tasks, and Oura sleep data",
      "orchestrator": "daily-brief-orchestrator",
      "agents": [
        "weather-fetcher",
        "calendar-fetcher",
        "email-processor",
        "task-compiler",
        "oura-fetcher",
        "quote-fetcher",
        "insight-generator",
        "note-creator"
      ],
      "steps": [
        {
          "group": 1,
          "parallel": true,
          "agents": [
            {
              "name": "weather-fetcher",
              "prompt": "Get current weather and 3-day forecast"
            },
            {
              "name": "calendar-fetcher",
              "prompt": "Retrieve today's calendar events"
            },
            {
              "name": "email-processor",
              "prompt": "Get important emails from last 24 hours"
            },
            {
              "name": "task-compiler",
              "prompt": "Compile and prioritize tasks"
            },
            {
              "name": "oura-fetcher",
              "prompt": "Get last night's sleep data"
            },
            {
              "name": "quote-fetcher",
              "prompt": "Get inspirational quote"
            }
          ]
        },
        {
          "group": 2,
          "parallel": false,
          "agents": [
            {
              "name": "insight-generator",
              "prompt": "Analyze collected data and generate actionable insights"
            }
          ]
        },
        {
          "group": 3,
          "parallel": false,
          "agents": [
            {
              "name": "note-creator",
              "prompt": "Create comprehensive Obsidian daily note with all data"
            }
          ]
        }
      ],
      "trigger": {
        "type": "scheduled",
        "schedule": "daily 8:00",
        "timezone": "Asia/Bangkok",
        "enabled": true
      },
      "output": {
        "type": "obsidian-note",
        "destination": "Daily/{{date}}.md"
      },
      "estimatedDuration": "10 seconds",
      "created": "2025-01-01T00:00:00.000Z",
      "version": "1.0.0",
      "lastRun": null,
      "runCount": 0,
      "successRate": 1
    },
    "life-evening-report": {
      "name": "evening-report",
      "displayName": "Evening Report",
      "department": "life",
      "description": "Analyze today's accomplishments and prepare tomorrow's briefing with weather and calendar",
      "orchestrator": "evening-report-orchestrator",
      "agents": [
        "note-reader",
        "accomplishment-analyzer",
        "tomorrow-weather",
        "tomorrow-calendar",
        "note-appender"
      ],
      "steps": [
        {
          "group": 1,
          "parallel": true,
          "agents": [
            {
              "name": "note-reader",
              "prompt": "Read today's daily note"
            },
            {
              "name": "tomorrow-weather",
              "prompt": "Get tomorrow's weather forecast"
            },
            {
              "name": "tomorrow-calendar",
              "prompt": "Get tomorrow's calendar events"
            }
          ]
        },
        {
          "group": 2,
          "parallel": false,
          "agents": [
            {
              "name": "accomplishment-analyzer",
              "prompt": "Analyze completed tasks and accomplishments from today's note"
            }
          ]
        },
        {
          "group": 3,
          "parallel": false,
          "agents": [
            {
              "name": "note-appender",
              "prompt": "Append evening summary with accomplishments and tomorrow's preview to today's note"
            }
          ]
        }
      ],
      "trigger": {
        "type": "manual",
        "enabled": true
      },
      "output": {
        "type": "obsidian-note",
        "destination": "Daily/{{date}}.md"
      },
      "estimatedDuration": "8 seconds",
      "created": "2025-01-01T00:00:00.000Z",
      "version": "1.0.0",
